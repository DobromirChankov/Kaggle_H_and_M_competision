    "test_customers = pd.read_csv('data\\sample_submission.csv\\sample_submission.csv')\n",
    "test_customers['article_id'] = ''\n",
    "\n",
    "# Предсказване на следващата покупка за всеки клиент\n",
    "def predict_next_purchase(model, X, customer_ids, num_articles):\n",
    "    \"\"\"\n",
    "    Предсказва вероятността за закупуване на всеки продукт за всеки клиент\n",
    "    за следващата седмица\n",
    "    \"\"\"\n",
    "    predictions = model.predict(X)\n",
    "    # създаване на речник с прогнозираните вероятности за всеки клиент и продукт\n",
    "    prediction_dict = {}\n",
    "    for i, cid in enumerate(customer_ids):\n",
    "        prediction_dict[cid] = predictions[i]\n",
    "    # избор на продуктите с най-висока вероятност за всеки клиент\n",
    "    recommended_products = {}\n",
    "    for cid, pred in prediction_dict.items():\n",
    "        recommended_products[cid] = [str(x) for x in np.argsort(pred)[-num_articles:]][::-1]\n",
    "    return recommended_products\n",
    "\n",
    "# Предсказване на следващата покупка за всички клиенти\n",
    "next_purchase = predict_next_purchase(my_rnn_model, X_test, test_customers, 5)\n",
    "\n",
    "# Създаване на DataFrame с препоръчани продукти за всеки клиент\n",
    "rec_products = pd.DataFrame(columns=['customer_id', 'article_id'])\n",
    "for cid, articles in next_purchase.items():\n",
    "    for article in articles:\n",
    "        rec_products = rec_products.append({'customer_id': cid, 'article_id': article}, ignore_index=True)\n",
    "\n",
    "# Записване на резултатите в CSV файл\n",
    "rec_products.to_csv('recommended_products.csv', index=False)\n"
   ]
