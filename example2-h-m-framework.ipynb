   "source": [
    "# Define batch size and number of epochs\n",
    "batch_size = 32\n",
    "num_epochs = 100\n",
    "\n",
    "# assuming y_test is our original label array of shape (n_samples,)\n",
    "# OH encoding in portion because of memory lack\n",
    "\n",
    "def label_generator(labels, batch_size, num_classes):\n",
    "    num_samples = len(labels)\n",
    "    while True:\n",
    "        indices = np.random.permutation(num_samples)\n",
    "        for i in range(0, num_samples, batch_size):\n",
    "            batch_indices = indices[i:i+batch_size]\n",
    "            batch_labels = np.zeros((batch_size, num_classes), dtype='int8')\n",
    "            for j, label in enumerate(labels[batch_indices]):\n",
    "                batch_labels[j, label] = 1\n",
    "            yield batch_labels\n",
    "\n",
    "# Wrapper function to feed the generator function to fit() method\n",
    "def label_generator_wrapper(labels, batch_size, num_classes):\n",
    "    gen = label_generator(labels, batch_size, num_classes)\n",
    "    while True:\n",
    "        yield next(gen)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
